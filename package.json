{
  "name": "convert",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist",
    "build": "tsc -p .",
    "postbuild": "npm run bundle",
    "clean": "find ./dist/ -type f -name '*.js' -delete && find ./dist/ -type f -name '*.js.map' -delete",
    "watch-serve": "nodemon -L --exec 'npm run start' ./dist",
    "prewatch": "npm run clean",
    "watch": " npm run build -- -w",
    "typings": "typings install",
    "bundle": "browserify './dist/public/client.js' -o './dist/public/bundle.js'"
  },
  "author": "Simon Gausmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/GauSim/Convert.git"
  },
  "license": "ISC",
  "devDependencies": {
    "typescript": "^1.8.7",
    "typings": "^0.7.9"
  },
  "dependencies": {
    "browserify": "^13.0.0",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "install": "^0.6.0",
    "jsdom": "^8.1.0",
    "nodemon": "^1.9.1",
    "npm": "^3.8.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "request": "^2.69.0",
    "universal-fetch": "0.0.1"
  }
}
